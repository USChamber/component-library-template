<svg id="svg" viewBox="0 0 100 100" xmlns="http://www.w3.org/2000/svg">

    <defs>
        <mask id="circlesTL">
            <rect x="0" y="0" width="100" height="100" fill="black"/>
            <circle cx="40" cy="40" r="30" fill="white"/>
            <circle cx="50" cy="50" r="10" fill="black"/>
        </mask>

        <mask id="circlesTR">
            <rect x="0" y="0" width="100" height="100" fill="black"/>
            <circle cx="60" cy="40" r="30" fill="white"/>
            <circle cx="50" cy="50" r="10" fill="black"/>
        </mask>

        <mask id="circlesBL">
            <rect x="0" y="0" width="100" height="100" fill="black"/>
            <circle cx="40" cy="60" r="30" fill="white"/>
            <circle cx="50" cy="50" r="10" fill="black"/>
        </mask>

        <mask id="circlesBR">
            <rect x="0" y="0" width="100" height="100" fill="black"/>
            <circle cx="60" cy="60" r="30" fill="white"/>
            <circle cx="50" cy="50" r="10" fill="black"/>
        </mask>

        <polygon id="strake" points="50,50 60,50 120,55 120,60 60,50 50,50"/>
        <!--<polygon id="strake" points="50,50 120,55 120,65 50,50" />-->

        <g id="strakes">
            <use href="#strake" style="transform: rotate(calc(var(--rotate) + 0deg)); transform-origin: var(--offset-x) var(--offset-y);"/>
            <use href="#strake" style="transform: rotate(calc(var(--rotate) + 12deg)); transform-origin: var(--offset-x) var(--offset-y);"/>
            <use href="#strake" style="transform: rotate(calc(var(--rotate) + 24deg)); transform-origin: var(--offset-x) var(--offset-y);"/>
            <use href="#strake" style="transform: rotate(calc(var(--rotate) + 36deg)); transform-origin: var(--offset-x) var(--offset-y);"/>
            <use href="#strake" style="transform: rotate(calc(var(--rotate) + 48deg)); transform-origin: var(--offset-x) var(--offset-y);"/>
            <use href="#strake" style="transform: rotate(calc(var(--rotate) + 60deg)); transform-origin: var(--offset-x) var(--offset-y);"/>
            <use href="#strake" style="transform: rotate(calc(var(--rotate) + 72deg)); transform-origin: var(--offset-x) var(--offset-y);"/>
            <use href="#strake" style="transform: rotate(calc(var(--rotate) + 84deg)); transform-origin: var(--offset-x) var(--offset-y);"/>
            <use href="#strake" style="transform: rotate(calc(var(--rotate) + 96deg)); transform-origin: var(--offset-x) var(--offset-y);"/>
            <use href="#strake" style="transform: rotate(calc(var(--rotate) + 108deg)); transform-origin: var(--offset-x) var(--offset-y);"/>
            <use href="#strake" style="transform: rotate(calc(var(--rotate) + 120deg)); transform-origin: var(--offset-x) var(--offset-y);"/>
            <use href="#strake" style="transform: rotate(calc(var(--rotate) + 132deg)); transform-origin: var(--offset-x) var(--offset-y);"/>
            <use href="#strake" style="transform: rotate(calc(var(--rotate) + 144deg)); transform-origin: var(--offset-x) var(--offset-y);"/>
            <use href="#strake" style="transform: rotate(calc(var(--rotate) + 156deg)); transform-origin: var(--offset-x) var(--offset-y);"/>
            <use href="#strake" style="transform: rotate(calc(var(--rotate) + 168deg)); transform-origin: var(--offset-x) var(--offset-y);"/>
            <use href="#strake" style="transform: rotate(calc(var(--rotate) + 180deg)); transform-origin: var(--offset-x) var(--offset-y);"/>
            <use href="#strake" style="transform: rotate(calc(var(--rotate) + 192deg)); transform-origin: var(--offset-x) var(--offset-y);"/>
            <use href="#strake" style="transform: rotate(calc(var(--rotate) + 204deg)); transform-origin: var(--offset-x) var(--offset-y);"/>
            <use href="#strake" style="transform: rotate(calc(var(--rotate) + 216deg)); transform-origin: var(--offset-x) var(--offset-y);"/>
            <use href="#strake" style="transform: rotate(calc(var(--rotate) + 228deg)); transform-origin: var(--offset-x) var(--offset-y);"/>
            <use href="#strake" style="transform: rotate(calc(var(--rotate) + 240deg)); transform-origin: var(--offset-x) var(--offset-y);"/>
            <use href="#strake" style="transform: rotate(calc(var(--rotate) + 252deg)); transform-origin: var(--offset-x) var(--offset-y);"/>
            <use href="#strake" style="transform: rotate(calc(var(--rotate) + 264deg)); transform-origin: var(--offset-x) var(--offset-y);"/>
            <use href="#strake" style="transform: rotate(calc(var(--rotate) + 276deg)); transform-origin: var(--offset-x) var(--offset-y);"/>
            <use href="#strake" style="transform: rotate(calc(var(--rotate) + 288deg)); transform-origin: var(--offset-x) var(--offset-y);"/>
            <use href="#strake" style="transform: rotate(calc(var(--rotate) + 300deg)); transform-origin: var(--offset-x) var(--offset-y);"/>
            <use href="#strake" style="transform: rotate(calc(var(--rotate) + 312deg)); transform-origin: var(--offset-x) var(--offset-y);"/>
            <use href="#strake" style="transform: rotate(calc(var(--rotate) + 324deg)); transform-origin: var(--offset-x) var(--offset-y);"/>
            <use href="#strake" style="transform: rotate(calc(var(--rotate) + 336deg)); transform-origin: var(--offset-x) var(--offset-y);"/>
            <use href="#strake" style="transform: rotate(calc(var(--rotate) + 348deg)); transform-origin: var(--offset-x) var(--offset-y);"/>
        </g>
    </defs>

    <style>
        :root {
            --rotate-base: 0deg;
            --rotate-tl: 0deg;
            --rotate-tr: 0deg;
            --rotate-bl: 0deg;
            --rotate-br: 0deg;
        }

        #strakes {
            --offset-x: 50%;
            --offset-y: 50%;
        }

        #strakesTL {
            --rotate: calc(var(--rotate-base) * 0.25);
        }

        #strakesTR {
            --rotate: calc(var(--rotate-base) * 1);
            stroke-width: 2;
            stroke: #000;
        }

        #strakesBL {
            --rotate: calc(var(--rotate-base) * 0.75);
            stroke-width: 2;
            stroke: #000;
        }

        #strakesBR {
            --rotate: calc(var(--rotate-base) * -0.5);
        }
    </style>

    <use id="strakesTL" href="#strakes" mask="url(#circlesTL)"/>
    <use id="strakesTR" href="#strakes" mask="url(#circlesTR)"/>
    <use id="strakesBL" href="#strakes" mask="url(#circlesBL)"/>
    <use id="strakesBR" href="#strakes" mask="url(#circlesBR)"/>
</svg>

<script>
    let angle = 0;

    function updateAngle() {
        angle += 0.25;
        document.getElementById('svg').style.setProperty('--rotate-base', `${angle}deg`);
        window.requestAnimationFrame(updateAngle);
    }

    updateAngle();
</script>

<style>
    #svg {
        max-width: 400px;
        height: auto;
    }
</style>
