    </g>
</svg>

<script>
    let angle = 0;

    function updateAngle() {
        angle += 0.1;
        document.getElementById('svg').style.setProperty('--rotate', `${angle}deg`);
        window.requestAnimationFrame(updateAngle);
    }

    updateAngle();
</script>
