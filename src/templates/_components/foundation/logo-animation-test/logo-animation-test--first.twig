<svg id="svg" viewBox="0 0 100 100" xmlns="http://www.w3.org/2000/svg">

    <defs>
        <mask id="circles">
            <rect x="0" y="0" width="100" height="100" fill="black"/>
            <circle cx="50" cy="50" r="50" fill="white"/>
            <circle cx="30" cy="30" r="15" fill="black"/>
        </mask>

        <polygon id="strake" points="30,30 45,30 120,35 120,45 45,30 30,30"/>
    </defs>

    <style>
        :root {
            --rotate: 0deg;
        }
    </style>

    <g id="strakes" stroke="black" mask="url(#circles)" fill="black" stroke-width="0">
        <use href="#strake" style="transform: rotate(calc(var(--rotate) + 0deg)); transform-origin: 30% 30%;"/>
        <use href="#strake" style="transform: rotate(calc(var(--rotate) + 12deg)); transform-origin: 30% 30%;"/>
